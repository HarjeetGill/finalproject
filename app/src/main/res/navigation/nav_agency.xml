<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_customer"
    app:startDestination="@+id/nav_request_list">

    <fragment
        android:id="@+id/nav_request_list"
        android:name="com.techai.shiftme.ui.agency.home.RequestsFragment"
        android:label="@string/menu_requests"
        tools:layout="@layout/fragment_requests" >
        <action
            android:id="@+id/action_nav_send_request_to_nav_profile"
            app:destination="@id/nav_profile" />
    </fragment>

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.techai.shiftme.ui.userprofile.UserProfileFragment"
        android:label="@string/menu_profile"
        tools:layout="@layout/fragment_user_profile" />

</navigation>